#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/PpisSuffering/FullStack/fullstack_term_project_g15/tp-testing/node_modules/.pnpm/sshpk@1.18.0/node_modules/sshpk/bin/node_modules:/mnt/c/PpisSuffering/FullStack/fullstack_term_project_g15/tp-testing/node_modules/.pnpm/sshpk@1.18.0/node_modules/sshpk/node_modules:/mnt/c/PpisSuffering/FullStack/fullstack_term_project_g15/tp-testing/node_modules/.pnpm/sshpk@1.18.0/node_modules:/mnt/c/PpisSuffering/FullStack/fullstack_term_project_g15/tp-testing/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/PpisSuffering/FullStack/fullstack_term_project_g15/tp-testing/node_modules/.pnpm/sshpk@1.18.0/node_modules/sshpk/bin/node_modules:/mnt/c/PpisSuffering/FullStack/fullstack_term_project_g15/tp-testing/node_modules/.pnpm/sshpk@1.18.0/node_modules/sshpk/node_modules:/mnt/c/PpisSuffering/FullStack/fullstack_term_project_g15/tp-testing/node_modules/.pnpm/sshpk@1.18.0/node_modules:/mnt/c/PpisSuffering/FullStack/fullstack_term_project_g15/tp-testing/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sshpk@1.18.0/node_modules/sshpk/bin/sshpk-conv" "$@"
else
  exec node  "$basedir/../../../../../sshpk@1.18.0/node_modules/sshpk/bin/sshpk-conv" "$@"
fi
